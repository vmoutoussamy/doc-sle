#: deployment_remote.xml:69 deployment_remote.xml:498
#, no-c-format
msgid "A repository, either remote or local:"
msgstr ""

#. Tag: para
#: deployment_remote.xml:74 deployment_remote.xml:503
#, no-c-format
msgid ""
"Remote repository: NFS, HTTP, FTP, TFTP, or SMB with working network "
"connection."
msgstr ""

#. Tag: para
#: deployment_remote.xml:80 deployment_remote.xml:509
#, no-c-format
msgid "Local repository, for example a DVD."
msgstr ""

