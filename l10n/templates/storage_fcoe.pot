#. Tag: screen
#: storage_fcoe.xml:443
#, no-c-format
msgid "sudo cp /etc/fcoe/cfg-ethx /etc/fcoe/cfg-eth2"
msgstr ""

#. Tag: para
#: storage_fcoe.xml:446
#, no-c-format
msgid "Start the FCoE Initiator service."
msgstr ""

#. Tag: screen
#: storage_fcoe.xml:449
#, no-c-format
msgid "systemctl start fcoe.service"
msgstr ""

